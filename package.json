{
  "name": "yesplaymusic",
  "version": "1.0.0",
  "private": true,
  "description": "This template should help get you started developing with Vue 3 in Vite.",
  "license": "ISC",
  "author": "yb",
  "type": "commonjs",
  "main": "src/background.js",
  "scripts": {
    "net": "node net/app.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "start": "electron .",
    "electron:dev": "concurrently \"vite --mode development\" \" node net/app.js \" \"wait-on http://localhost:3000 && electron .\"",
    "lint-fix": "eslint --fix \"**/*.{js,ts}\"",
    "prepare": "husky install",
    "pkgwin": "pkg . -t node18-win-x64 -C GZip -o bin/app --no-bytecode",
    "pkglinux": "pkg . -t node18-linux-x64 -C GZip -o bin/app --no-bytecode",
    "pkgmacos": "pkg . -t node18-macos-x64 -C GZip -o bin/app --no-bytecode"
  },
  "bin": "net/app.js",
  "pkg": {
    "scripts": "net/module/*.js",
    "assets": [
      "node_modules/axios",
      "node_modules/express",
      "node_modules/express-fileupload",
      "node_modules/md5",
      "node_modules/music-metadata",
      "node_modules/pac-proxy-agent",
      "node_modules/qrcode",
      "node_modules/safe-decode-uri-component",
      "node_modules/tunnel",
      "node_modules/yargs",
      "node_modules/tslib",
      "node_modules/vm2",
      "net/module",
      "net/public",
      "net/data"
    ]
  },
  "keywords": [
    "网易云音乐",
    "网易云",
    "音乐",
    "网易云音乐nodejs"
  ],
  "net_main": "net/main.js",
  "types": "net/interface.d.ts",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "files": [
    "net/module",
    "net/util",
    "net/plugins",
    "net/main.d.ts",
    "net/interface.d.ts",
    "net/module_types",
    "net/server.js",
    "net/generateConfig.js",
    "net/public",
    "net/data"
  ],
  "dependencies": {
    "@applemusic-like-lyrics/core": "^0.2.0",
    "@applemusic-like-lyrics/vue": "^0.2.0",
    "@electron/remote": "^2.1.3",
    "@neteaseapireborn/api": "^4.29.13",
    "@pixi/app": "^7.4.3",
    "@pixi/core": "^7.4.3",
    "@pixi/display": "^7.4.3",
    "@pixi/filter-blur": "^7.4.3",
    "@pixi/filter-bulge-pinch": "^5.1.1",
    "@pixi/filter-color-matrix": "^7.4.3",
    "@pixi/sprite": "^7.4.3",
    "@vueuse/core": "^14.0.0",
    "element-plus": "^2.11.8",
    "gsap": "^3.13.0",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "motion-v": "^1.7.4",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@playwright/test": "^1.56.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.37.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "~10.5.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "naive-ui": "^2.43.1",
    "prettier": "3.6.2",
    "sass-embedded": "^1.93.2",
    "vfonts": "^0.0.3",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.1"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
